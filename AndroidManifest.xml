<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.qualit"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="15"
        android:targetSdkVersion="19" />

    <uses-permission android:name="android.permission.SEND_SMS" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.qualit.WelcomeActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.qualit.SetActivity"
            android:label="Select No of Cups"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.example.qualit.SettingsActivity"
            android:label="@string/title_activity_settings" >
        </activity>
        <activity
            android:name="com.example.qualit.Setting"
            android:label="@string/title_activity_setting" >
        </activity>
        <activity
            android:name="com.example.qualit.DialogFrag"
            android:label="@string/title_activity_dialog"
            android:parentActivityName="com.example.cima_onthego.Dialog" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.cima_onthego.Dialog" />
        </activity>
        <activity
            android:name="com.example.qualit.Thank"
            android:label="@string/title_activity_thank" 
            android:screenOrientation="portrait">
        </activity>
    </application>

</manifest>
